{
    "name": "tiptap-editor-workspace",
    "version": "1.0.0",
    "description": "TipTap v3.x editor implementation",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
        "type-check": "tsc --noEmit",
        "test": "vitest --config test/vitest.config.ts",
        "test:ui": "vitest --ui --config test/vitest.config.ts",
        "test:coverage": "vitest --coverage --config test/vitest.config.ts",
        "test:e2e": "playwright test --config test/playwright.config.ts",
        "test:e2e:ui": "playwright test --ui --config test/playwright.config.ts",
        "test:e2e:headed": "playwright test --headed --config test/playwright.config.ts",
        "test:all": "npm run test -- --run && npm run test:e2e"
    },
    "dependencies": {
        "@floating-ui/react": "^0.27.16",
        "@heroicons/react": "^2.2.0",
        "@radix-ui/react-dropdown-menu": "^2.1.16",
        "@radix-ui/react-popover": "^1.1.15",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-slider": "^1.3.6",
        "@radix-ui/react-toggle-group": "^1.1.11",
        "@tiptap/core": "^3.7.2",
        "@tiptap/extension-blockquote": "^3.7.2",
        "@tiptap/extension-bold": "^3.7.2",
        "@tiptap/extension-bubble-menu": "^3.7.2",
        "@tiptap/extension-bullet-list": "^3.7.2",
        "@tiptap/extension-code": "^3.7.2",
        "@tiptap/extension-code-block": "^3.7.2",
        "@tiptap/extension-color": "^3.7.2",
        "@tiptap/extension-document": "^3.7.2",
        "@tiptap/extension-dropcursor": "^3.7.2",
        "@tiptap/extension-floating-menu": "^3.7.2",
        "@tiptap/extension-font-family": "^3.7.2",
        "@tiptap/extension-gapcursor": "^3.7.2",
        "@tiptap/extension-hard-break": "^3.7.2",
        "@tiptap/extension-heading": "^3.7.2",
        "@tiptap/extension-highlight": "^3.7.2",
        "@tiptap/extension-history": "^3.7.2",
        "@tiptap/extension-horizontal-rule": "^3.7.2",
        "@tiptap/extension-image": "^3.7.2",
        "@tiptap/extension-italic": "^3.7.2",
        "@tiptap/extension-link": "^3.7.2",
        "@tiptap/extension-list-item": "^3.7.2",
        "@tiptap/extension-mathematics": "^3.7.2",
        "@tiptap/extension-ordered-list": "^3.7.2",
        "@tiptap/extension-paragraph": "^3.7.2",
        "@tiptap/extension-placeholder": "^3.7.2",
        "@tiptap/extension-strike": "^3.7.2",
        "@tiptap/extension-table": "^3.7.2",
        "@tiptap/extension-table-cell": "^3.7.2",
        "@tiptap/extension-table-header": "^3.7.2",
        "@tiptap/extension-table-row": "^3.7.2",
        "@tiptap/extension-task-item": "^3.7.2",
        "@tiptap/extension-task-list": "^3.7.2",
        "@tiptap/extension-text": "^3.7.2",
        "@tiptap/extension-text-align": "^3.7.2",
        "@tiptap/extension-text-style": "^3.7.2",
        "@tiptap/extension-underline": "^3.7.2",
        "@tiptap/extension-youtube": "^3.7.2",
        "@tiptap/pm": "^3.7.2",
        "@tiptap/react": "^3.7.2",
        "@tiptap/starter-kit": "^3.7.2",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cors": "^2.8.5",
        "express": "^4.21.2",
        "geist": "^1.5.1",
        "katex": "^0.16.25",
        "loglevel": "^1.9.2",
        "loglevel-plugin-remote": "^0.6.8",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "tailwind-merge": "^3.3.1"
    },
    "devDependencies": {
        "@playwright/test": "^1.56.1",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/node": "^22.18.11",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "@typescript-eslint/eslint-plugin": "^8.46.1",
        "@typescript-eslint/parser": "^8.46.1",
        "@vitejs/plugin-react": "^5.0.4",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "eslint": "^9.38.0",
        "eslint-plugin-react-hooks": "^5.1.0",
        "eslint-plugin-react-refresh": "^0.4.17",
        "jsdom": "^27.0.1",
        "playwright": "^1.56.1",
        "prettier": "^3.6.2",
        "typescript": "^5.9.3",
        "vite": "^7.1.11",
        "vitest": "^3.2.4"
    },
    "engines": {
        "node": ">=20.19.0",
        "npm": ">=10.0.0"
    }
}
